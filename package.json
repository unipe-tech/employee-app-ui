  "scripts": {
    "start:dev": "cp ./.env.dev ./.env && npx react-native run-android",
    "start:release": "cp ./.env.release ./.env && npx react-native run-android",
    "buildRelease": "cp ./.env.release ./.env && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleRelease",
    "buildDebug": "cp ./.env.dev ./.env && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
